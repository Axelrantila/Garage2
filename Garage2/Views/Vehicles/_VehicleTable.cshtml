@model IEnumerable<Garage2.Models.Vehicle>

<table class="table">
    <tr>
        <th>
            <label>Parking lot</label>
        </th>
        <th>
            <label>Owner</label>
        </th>
        <th>
            <label>License Number</label>
        </th>
        <th>
            <label>Type of Vehicle</label>
        </th>
        <th>
            <label>Length(m)</label>
        </th>
        <th>
            <label>Weight(kg)</label>
        </th>
        <th>
            <label>Maker/Mode</label>
        </th>
        <th>
            <label>Color</label>
        </th>
        <th>
            <label>Number of Wheels</label>
        </th>
        <th>
            <label>Parking date</label>
        </th>
        <th>
            <label>Currently parked</label>
        </th>
        <th></th>
    </tr>

    @foreach ( var item in Model ) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ParkingLot)
            </td>
            <td>
                @Html.DisplayFor( modelItem => item.Owner )
            </td>
            <td>
                @Html.DisplayFor( modelItem => item.LicenseNr )
            </td>
            <td>
                @Html.DisplayFor( modelItem => item.TypeOfVehicle )
            </td>
            <td>
                @Html.DisplayFor( modelItem => item.Length )
            </td>
            <td>
                @Html.DisplayFor( modelItem => item.Weight )
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MakeAndModel)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Color)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.NrOfWheels)
            </td>

            @Html.Partial("_ParkedTableData", item)
        </tr>
    }
    </>

</table>


