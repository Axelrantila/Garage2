@model Garage2.Models.Vehicle

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>Vehicle</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <label>Owner</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Owner)
        </dd>

        <dt>
            <label>License number</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LicenseNr)
        </dd>

        <dt>
            <label>Type of vehicle</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TypeOfVehicle)
        </dd>

        <dt>
            <label>Length(m)</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Length)
        </dd>

        <dt>
            <label>Weight(kg)</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Weight)
        </dd>

        <dt>
            <label>Time parked</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TimeParked)
        </dd>

        <dt>
            <label>Parked</label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Parked)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("Back to List", "Index")
